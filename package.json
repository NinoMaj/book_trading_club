{
  "name": "book_trading_club",
  "version": "1.0.0",
  "description": "Manage book trading club, FCC challange",
  "main": "index.js",
  "browserslist": [
    "> 1%"
  ],
  "engines": {
    "node": "8.0.0",
    "npm": "5.0.0"
  },
  "scripts": {
    "start": "npm run dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": " webpack-dev-server --progress",
    "prod:start": "heroku local",
    "heroku-postbuild": "npm run prod:build",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "eslint src && flow && jest --coverage",
    "test:no-flow": "eslint src && jest --coverage",
    "precommit": "npm run test:no-flow",
    "prepush": "npm run test:no-flow && npm run prod:build"
  },
  "author": "Nino Maj",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-flow-react-proptypes": "^2.2.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-compat": "^1.0.3",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.46.0",
    "husky": "^0.13.3",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.0.0-alpha.6",
    "caniuse-lite": "^1.0.30000676",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "has-flag": "^2.0.0",
    "is-utf8": "^0.2.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^7.4.1",
    "locate-path": "^2.0.0",
    "material-ui": "^0.18.1",
    "mongoose": "^4.10.4",
    "node-pre-gyp": "^0.6.36",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^1.7.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.0.0",
    "tether": "^1.4.0",
    "universal-cookie": "^2.0.8",
    "validator": "^7.0.0"
  }
}
